{
  "manifest_version": 3,
  "name": "Supremify",
  "version": "0.0.1",
  "description": "Turn every instance of the word 'supreme' into the Supreme logo",
  "host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "16": "emblem_16.png",
    "32": "emblem_32.png",
    "48": "emblem_48.png",
    "128": "emblem_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ]
}
